            <!-- One -->
            <section id="one" class="tiles">
                <article>
                    <span class="image">
                        <img src="https://unsplash.com/photos/JmuyB_LibRo" alt=""/>
                    </span>
                    <header class="major">
                        <h3><a href="landing.html" class="link">Traditional Wear</a></h3>
